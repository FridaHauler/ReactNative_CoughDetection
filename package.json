{
  "name": "CoughRecording",
  "version": "1.0.4",
  "private": true,
  "scripts": {
    "postinstall": "yarn pod",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "pod": "cd ios && pod install || echo '\n*** CocoaPods not found, iOS build will not work!! ***\n' && cd ..",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "xcode": "open ios/CoughRecording.xcworkspace/",
    "generateIcons": "yarn add @bam.tech/react-native-make && react-native set-icon --path src/assets/logo.png --background \"#4b4c50\" && yarn remove @bam.tech/react-native-make"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@react-native-community/masked-view": "^0.1.10",
